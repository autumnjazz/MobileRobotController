package mappackage;

import spotpackage.*;

import java.util.ArrayList;

import robotpackage.Robot;

public class DynamicMap {
	Map m;
	private Spot board[][];
	public ArrayList visited = new ArrayList();
	public DynamicMap(Map m) {
		this.m = m;
		board = m.board;
	}
	public void printBoard() {
		for (int i = 0; i < m.row; i++) {
			for (int j = 0; j< m.col; j++) {
				System.out.print(board[i][j].getCharacter());
			}
			System.out.println();
		}
	}
	public void updateBoard(int x, int y, Spot s) {
		board[x][y] = s;
	}
}
